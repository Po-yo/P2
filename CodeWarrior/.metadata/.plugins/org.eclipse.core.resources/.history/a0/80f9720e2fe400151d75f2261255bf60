typedef enum
{
    LIGHTS_OFF = 0,	//“Parklamps” y “Headlamps” apagadas
    LIGHTS_HALF,	//“Parklamps” prendida y “Headlamps” apagada
    LIGHTS_ON,		//“Parklamps” y “Headlamps” prendidas
}E_LAMP;


